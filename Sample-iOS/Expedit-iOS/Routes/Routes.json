{
    "Comments": {
        "URI": "/article/:articleId/comments",
        "requirements": {
            "articleId": "\\d+"
        },
        "controller": "CommentsVC"
    },
    "Article": {
        "URI": "article/:articleId/",
        "requirements": {
            "articleId": "\\d+"
        },
        "controller": "ArticleVC"
    },
    "List": {
        "URI": "list/:filter?",
        "requirements": {
            "filter": "true|plip"
        },
        "controller": "ListVC"
    },
    "Credit": {
        "URI": "credit",
        "controller": "Credit.CreditVC"
    },
    "Manual": {
        "URI": "manual"
    }
}
